@model IEnumerable<ToDoBook.Storage.Entity.News>

<!DOCTYPE html>

<html>
<head>
	<title>User's Diary</title>

</head>
<body>
	<header class="header">
		<div class="header-logo">ToDoBook</div>
		<div class="container-logo">
			<img src="image/book.svg" alt="">
		</div>
		<nav class="nav">
			<a class="nav_link" href="/Home">Главная</a>
			<a class="nav_link" href="~/User">Профиль</a>
			<a class="nav_link" href="~/User/RequestDiary">Дневники</a>
			<a class="nav_link" href="/Home">О нас</a>
			<a class="nav_link" href="~/User/News">Новости</a>
		</nav>
	</header>

	<div class="mainstr">
		<article class="article1">
	<table>
		<tr>
			<td>Новость</td>
			<td></td>
		</tr>
		@foreach (var news in Model)
		{
			<tr>
				<td>@news.Description</td>
				<td>
					<input type="button" value="Перейти" onClick='location.href="@news.Url"'>
				</td>
			</tr>
		}
	</table>
		</article>
		<div class="footer"><div class="copy">Все права защищены &copy; 2021 ToDoBook</div></div>
	</div>

</body>

</html>